.list {
  background: #fff;
}
.list-item {
  overflow: hidden;
  border-bottom: 1px solid #ddd;
  padding: 20rpx 30rpx;
}
.list-img {
  float: left;
  border: 1px solid #eee;
  width: 159rpx;
  height: 159rpx;
}
.list-info {
  float: left;
  margin-left: 20rpx;
  width: 509rpx;
}
.list-title {
  margin-bottom: 5rpx;
  color: #000;
  font: bold 30rpx/40rpx '宋体';
}
.list-score {
  float: left;
  overflow: hidden;
  margin-top: 7rpx;
  width: 130rpx;
  height: 26rpx;
  background-image: url('data:image/png;base64,');
  background-size: auto 100%;
}
.list-score .score {
  display: inline-block;
  vertical-align: top;
  height: 100%;
  background-image: url('data:image/png;base64,');
  background-size: auto 100%;
}
.list-price {
  float: left;
  margin-left: 12rpx;
  color: #000;
  font-size: 26rpx;
  line-height: 40rpx;
}
.list-position {
  position: relative;
  color: #999;
  font-size: 26rpx;
  line-height: 42rpx;
}
.classify {
  margin-left: 20rpx;
}
.list-dis {
  position: absolute;
  top: 0;
  right: 0;
}
.label text {
  float: left;
  margin-top: 10rpx;
  margin-right: 10rpx;
  border: 1px solid #ddd;
  padding: 0 10rpx;
  color: #707070;
  font: 22rpx/40rpx '宋体';
}
.loading-more {
  color: #333;
  font-size: 30rpx;
  line-height: 60rpx;
  text-align: center;
}